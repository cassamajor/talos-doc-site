    "typeface-play": "^0.0.54",
    "typeface-poppins": "^0.0.72",
    "typeface-roboto-condensed": "^0.0.54",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-subresource-integrity": "^1.3.2"

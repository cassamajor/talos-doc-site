    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "file-loader": "^3.0.1",
    "highlight.js": "^9.15.8",
    "intersection-observer": "^0.6.0",
    "lazysizes": "^4.1.8",
    "mini-css-extract-plugin": "^0.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.14",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "quicklink": "^1.0.0",
    "tailwindcss": "^1.0.2",
    "tailwindcss-aspect-ratio": "^1.0.3",
